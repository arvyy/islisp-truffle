(defun print (int)
    (format-integer (standard-output) int 10)
    (format-char (standard-output) #\newline))

(let ((p (cons 1 2)))
    (print (car p))
    (print (cdr p))
    (set-car p 3)
    (set-cdr p 4)
    (print (car p))
    (print (cdr p))
    (setf (car p) 5)
    (setf (cdr p) 6)
    (print (car p))
    (print (cdr p)))